{% extends 'base.html.twig' %}
{% block body %}
<form method="post" action="{{ path('projects_delete', {'gitLabId': project.gitLabId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ project.gitLabId) }}">
    <button class="btn">Delete</button>
</form>
{% endblock %}